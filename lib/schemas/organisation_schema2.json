{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "string",
      "title": "The Id Schema ",
      "default": "",
      "examples": [
        "1"
      ]
    },
    "resources": {
      "$id": "/properties/resources",
      "type": "object",
      "properties": {
        "avatar": {
          "$id": "/properties/resources/properties/avatar",
          "type": "object",
          "properties": {
            "allowed": {
              "$id": "/properties/resources/properties/avatar/properties/allowed",
              "type": "array",
              "items": {
                "$id": "/properties/resources/properties/avatar/properties/allowed/items",
                "type": "string",
                "title": "The 0th Schema ",
                "default": "",
                "examples": [
                  "DELETE",
                  "PUT",
                  "GET"
                ]
              }
            },
            "ref": {
              "$id": "/properties/resources/properties/avatar/properties/ref",
              "type": "string",
              "title": "The Ref Schema ",
              "default": "",
              "examples": [
                "http://dev188.sure.vine/api/core/v3/organisations/1/logo?a=-1"
              ]
            }
          }
        }
      }
    },
    "bio": {
      "$id": "/properties/bio",
      "type": "string",
      "title": "The Bio Schema ",
      "default": "",
      "examples": [
        "Surevine builds secure, scalable collaboration solutions for the most security conscious organisations; joining people up and enabling collaboration on their most highly sensitive information."
      ]
    },
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": [
        "Surevine"
      ]
    },
    "size": {
      "$id": "/properties/size",
      "type": "integer",
      "title": "The Size Schema ",
      "default": 0,
      "examples": [
        20
      ]
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "title": "The Email Schema ",
      "default": "",
      "examples": [
        "sales@surevine.com"
      ]
    },
    "telephone": {
      "$id": "/properties/telephone",
      "type": "string",
      "title": "The Telephone Schema ",
      "default": "",
      "examples": [
        "+44 845 468 1066"
      ]
    },
    "website": {
      "$id": "/properties/website",
      "type": "string",
      "title": "The Website Schema ",
      "default": "",
      "examples": [
        "http://www.surevine.com"
      ]
    },
    "type": {
      "$id": "/properties/type",
      "type": "string",
      "title": "The Type Schema ",
      "default": "",
      "examples": [
        "organisation"
      ]
    }
  }
}
  "additionalProperties": false
}